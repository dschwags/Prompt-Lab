# Prompt Lab Environment Variables
# Copy this file to .env and fill in the values

# Password protection (shared password for you + friends)
PROMPT_LAB_PASSWORD=your-secure-password-here

# Comma-separated list of allowed project names
# These must match folder names in /home/runner/workspace/
ALLOWED_PROJECTS=slyce,tapestr-ai

# Session configuration (optional)
SESSION_SECRET=change-this-to-a-random-string
SESSION_DAYS=7

# API Configuration (if using external APIs)
# ANTHROPIC_API_KEY=your-anthropic-key
# OPENAI_API_KEY=your-openai-key
